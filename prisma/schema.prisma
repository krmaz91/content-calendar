generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Project {
  id         String   @id @default(cuid())
  name       String
  websiteUrl String
  input      String
  sitemap    String
  calendar   String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
